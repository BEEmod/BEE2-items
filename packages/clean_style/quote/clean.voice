		}
		
		"Quote"
		{
			"Has" "CubeNotComp"
			"MidInst"
			{
				"Name" "Cube Fizzled"
				"Trans" "Oh well. We have warehouses FULL of the things. Absolutely worthless. I'm happy to get rid of them."
				"Trans" "(If dropper): Go ahead and grab another one."
				"File" "clean/mid_cube.vmf"
			}
		}
		
		"Quote"
		{
			"Has" "CubeNotComp"
			"MidInst"
			{
				"Name" "Cube Fizzled"
				"Trans" "Oh well. We have warehouses FULL of the things. Absolutely worthless. I'm happy to get rid of them."
